function func() {
    let a = [1, 2, 3, 4, 5, 6];
    let result = "";

    for (let i = 0; i < a.length; i++) {
        result += a[i];
        if (i < a.length - 1) {
            result += '|';
        }
    }

    console.log(result);
}

func();

// 1|2|3|4|5|6


