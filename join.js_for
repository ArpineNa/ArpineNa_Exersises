const matrix = [
    [1, 2, 3, 4, 5, 6],
    ["cars", "fruits", "Wind", "Water", "Fire", "snow"],
  ];  

function joinMatrixWithSeparator(matrix, separator) {
    const joinedArrays = [];
  
    for (let i = 0; i < matrix.length; i++) {
      const joinedSubarray = matrix[i].join(separator);
      joinedArrays.push(joinedSubarray);
    }
  
    return joinedArrays;
  }
      
  const separator = '|';
  const joinedMatrix = joinMatrixWithSeparator(matrix, separator);
  
  for (let i = 0; i < joinedMatrix.length; i++) {
    console.log(joinedMatrix[i]);
  }
// 1|2|3|4|5|6
cars|fruits|Wind|Water|Fire|snow
