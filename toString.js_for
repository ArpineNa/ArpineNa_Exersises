function arrayToString(arr) {
    let result = '';
    for (let i = 0; i < arr.length; i++) {
      result += arr[i].toString();
      if (i < arr.length - 1) {
        result += '/ '; 
      }
    }
    return result;
  }
  
  const numbers = [1, 2, 3, 4, 5, 6];
  
  const stringResult = arrayToString(numbers);
  console.log(stringResult);
// 1/ 2/ 3/ 4/ 5/ 6

